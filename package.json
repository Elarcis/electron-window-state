{
  "name": "@elarcis/electron-window-state",
  "version": "5.1.1",
  "description": "Simple module that helps to save and restore size and position of Electron windows.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elarcis/electron-window-state.git"
  },
  "main": "index.js",
  "author": "Marcel Wiehle, Elarcis",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "keywords": [
    "electron"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "jsonfile": "^6.1.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "mockery": "^2.1.0",
    "sinon": "^6.1.5",
    "xo": "^0.22.0"
  },
  "xo": {
    "space": true,
    "ignores": [
      "index.d.ts"
    ],
    "rules": {
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "prefer-object-spread": 0
    }
  }
}
